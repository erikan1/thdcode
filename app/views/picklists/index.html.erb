<p id="notice"><%= notice %></p>

<h1>Picklists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @picklists.each do |picklist| %>
      <tr>
        <td><%= link_to 'Show', picklist %></td>
        <td><%= link_to 'Edit', edit_picklist_path(picklist) %></td>
        <td><%= link_to 'Destroy', picklist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Picklist', new_picklist_path %>
